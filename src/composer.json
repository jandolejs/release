{
    "config": {
      "platform": {
        "php": "8.0.0",
        "ext-something": "8.0.0"
      },
        "allow-plugins": {
            "symfony/thanks": true,
            "php-http/discovery": true
        }
    },
	"name": "nette/web-project",
	"description": "Nette: Standard Web Project",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"require": {
      "php": ">= 8.0",
      "nette/application": "^3.1",
      "nette/bootstrap": "^3.1",
      "nette/caching": "^3.1",
      "nette/database": "^3.1",
      "nette/di": "^3.0",
      "nette/finder": "^2.5",
      "nette/forms": "^3.1",
      "nette/http": "^3.1",
      "nette/mail": "^3.1",
      "nette/robot-loader": "^3.3",
      "nette/security": "^3.1",
      "nette/utils": "^3.2",
      "latte/latte": "^2.9",
      "tracy/tracy": "^2.8",
      "ext-curl": "*",
      "ext-libxml": "*",
      "ext-mbstring": "*",
      "ext-dom": "*",
      "marufmax/emoticon-php": "*",
      "knplabs/github-api": "*",
        "symfony/http-client": "^5.4",
        "nyholm/psr7": "^1.5"
